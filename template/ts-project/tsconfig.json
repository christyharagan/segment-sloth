{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2015",
    "sourceMap": false,
    "preserveConstEnums": true,
    "noEmit": true,
    "moduleResolution": "node",
    "outDir": "./out",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "typeRoots": ["./typings", "./node_modules/@types"],
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["./out"]
}
